@page
@using Ftl.SalesCrm.Localization;
@using Microsoft.Extensions.Localization;
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal;
@model Ftl.SalesCrm.Web.Pages.Contacts.EditModalModel
@inject IStringLocalizer<SalesCrmResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Contact" asp-page="/Contacts/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["Update"]"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>